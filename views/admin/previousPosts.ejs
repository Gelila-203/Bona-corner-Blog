

<div class="admin-title">
    <h2>View/Edit Old post</h2> 
    <a href="/add-post" class="btn"> + Add New Post</a>

</div>

<div>
    <a href="/dashboard" class="">Back to Dashboard </a>

</div>
</br>
<ul class="admin-posts">

    <% data.forEach(post=> {
        %>

        <li>
            <a href="/post/<%= post._id %>">

                <%= post.title %> &nearr;
            </a>
            <div class="admin-post-controls">
                <a href="/edit-post/<%= post._id %>" class="btn"> Edit </a>
                <form action="/delete-post/<%= post._id %>?_method=DELETE " method="POST">
                    
                    <input type="submit" value="Delete" class=" btn">
                </form>


            </div>
            

        </li>

        <% }) %>
        </br>
        <a href="/dashboard">&larr; Back </a>


</ul>


</br>

